<view class="question" wx:for="{{guanguan_list}}" wx:key="id" data-guan_id='{{item.id}}' bindtap="click_guan_info">
  <view class="question-title">
    {{item.name}}
  </view>
  <view class="question-info"> 
    <view class="info classification">{{item.guan_type}}</view>
    <view class="info point">{{item.guan_point}}</view>
    <view class="info answers">{{item.answers}}</view> 
  </view>
</view> 
