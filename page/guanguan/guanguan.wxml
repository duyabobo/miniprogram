<navigator class="question" wx:for="{{guanguan_list}}" wx:key="id" data-id='{{item.id}}' url="/page/guan_info/guan_info?guan_id={{item.id}}">
  <view class="question-title">
    {{item.name}}
  </view>
  <view class="question-info">
    <view class="static-info">
      <view class="info classification">{{item.guan_type}}</view>
      <view class="info point">{{item.guan_point}}</view>
    </view>
    <view class="dynamic-info">
      <view class="info answers">{{item.answers}}</view> 
    </view>
  </view>
</navigator> 
