.expect-info {
  margin: 70rpx 0rpx; 
  width: 100%;
  height: 90%;
  display: flex;
  flex-direction: column;
  overflow: scroll;
}
.info-list {
  display: flex;
  flex-direction: column; 
}
.expect-info-item {
  display: flex;
  flex-direction: row;
  margin: 25rpx 0 5rpx 0;
}
.line{
  margin: 0 auto;
  width: 60%;
  height: 4rpx;
  background-color: #D8D8D8;
}
.info-key {
  margin-right: 60rpx;  
  font-size: 35rpx;
  display: flex;
  color: #FF6600;
  flex: 1;
  justify-content: flex-end;
}
.info-value {
  margin-left: 60rpx; 
  font-size: 35rpx;
  display: flex;
  color: #FF6600;
  flex: 1;
  justify-content: flex-start;
  text-decoration:underline;
}
